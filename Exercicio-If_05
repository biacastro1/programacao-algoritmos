Scripts.js
//Elabore um algoritmo que leia 3 valores inteiros e mostre o maior valor entre eles.//

var num1 = parseInt(prompt("Digite o primeiro número"));
var num2 = parseInt(prompt("Digite o segundo número"));
var num3 = parseInt(prompt("Digite o terceiro número"));

if ( num1 > num2 && num1 > num3) {
    alert("O maior número é o primeiro: " + num1);
} else if (num2 > num1 && num2 > num3) {
    alert("O maior número é o segundo: " + num2)
} else if (num3 > num1 && num3 > num2) {
    alert("O terceiro número é o maior: " + num3)
}
