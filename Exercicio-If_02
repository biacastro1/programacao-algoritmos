Scripts.js
//Elaborar um algoritmo que informe se um ano informado pelo usuário é bissexto ou não.//
var ano = parseInt(prompt("Digite o ano"));

if(ano % 4 == 0) {
    alert("É Bissexto " + ano);
 } else {
    alert("Não é Bissexto " + ano);
 }
