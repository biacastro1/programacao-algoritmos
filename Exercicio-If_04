Scripts.js
//Elaborar um algoritmo que lê 2 valores a e b e os escreve com a mensagem: "São múltiplos" ou "Não são múltiplos". //

var a = parseInt(prompt("Digite o primeiro valor"));
var b = parseInt(prompt("Digite o segundo valor"));


if (a % b == 0) {
    alert("São Múltiplos " + a);
} else {
    alert("Não são Múltiplos " + b);
}
